CC	Makefile	/^CC       = $(CROSS_COMPILE)gcc$/;"	m
CFLAGS	Makefile	/^CFLAGS   = -c -g $/;"	m
CROSS_COMPILE	Makefile	/^CROSS_COMPILE=$/;"	m
EXECUTABLE	Makefile	/^EXECUTABLE = .\/testmakefile$/;"	m
INCLUDES	Makefile	/^INCLUDES = -I.\/$/;"	m
LDFLAGS	Makefile	/^LDFLAGS  = $/;"	m
OBJECTS	Makefile	/^OBJECTS  = $(SOURCES:.c=.o)$/;"	m
SOURCES	Makefile	/^SOURCES  = .\/hellofunc.c .\/hellomake.c$/;"	m
__HELLOW	hellomake.h	5;"	d
main	hellomake.c	/^int main() {$/;"	f
myPrintHelloMake	hellofunc.c	/^void myPrintHelloMake(void) {$/;"	f
